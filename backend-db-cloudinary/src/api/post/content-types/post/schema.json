{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Subtitle": {
      "type": "string",
      "required": true
    },
    "Thumbnail": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "Gallery": {
      "type": "component",
      "repeatable": false,
      "component": "elements.gallery",
      "required": true
    },
    "Tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "Collaborators": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::collaborator.collaborator"
    },
    "Code": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "Design": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
